<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<exercises>
    <!-- Exercise 1 -->
    <exercise name="TestEx">
        <description>Description comes here</description>
        <!-- Enter classes needed for the exercise, each class needs to be surrounded by a class tag with a name value -->
        <classes>
            <class name="TestClass">
                testable ClassCode
            </class>
            <class name="TestClass2">
                testable ClassCode2
            </class>
        </classes>
        <!-- Here comes the test code. -->
        <tests>
            <test name="TestExTester">
                Testable testtext
            </test>
            <test name="TestExTester2">
                Testable testtext2
            </test>
        </tests>
        <!-- Set config values for the exercise. -->
        <config>
            <!-- value may only contain a boolean value [TRUE / FALSE]. Time is measured in seconds. -->
            <babysteps time="10" value="true"/>
            <!-- value may only contain a boolean value [TRUE / FALSE] -->
            <timetracking value="True"/>
        </config>
    </exercise>

    <!-- Exercise 2 -->
    <exercise name="TestEx2">
        <description>Description comes here2</description>

        <classes>
            <class name="TestClass2.1">
                Testable classcode2.1
            </class>
        </classes>
        <tests>
            <test name="ex2test">
                Testable testtext2.1
            </test>
        </tests>

        <config>
            <!-- value may only contain a boolean value [TRUE / FALSE]. Time is measured in seconds. -->
            <babysteps time="20" value="False"/>
            <!-- value may only contain a boolean value [TRUE / FALSE] -->
            <timetracking value="false"/>
        </config>
    </exercise>
